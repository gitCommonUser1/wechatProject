<!--pages/list/list.wxml-->
<!-- 开始 -->
<view class = "listView" wx:for="{{list}}">
<view class="userInfo"> 
        <image class='userHead' src="{{item.userHead}}"></image>
        <text class="userText">编号：{{item.userNumber}}\n</text>
        <text class="userText">姓名：{{item.userName}}\n</text>
        <text class="userText">热度：{{item.userFire}}\n</text>
        <text class="userText">排名：{{item.userList}}\n</text>
</view>
<!-- 结束 -->
</view>